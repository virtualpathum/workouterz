<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.lk.infinitx.workouterz">

    <queries>
        <intent>
            <action android:name="android.inten.action.TTS_SERVICE" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:name=".WorkouterzApp"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WorkouterZ"
        tools:targetApi="31">
        <activity
            android:name="com.lk.infinitx.workouterz.FinishActivity"
            android:exported="false" />
        <activity
            android:name="com.lk.infinitx.workouterz.ExcerciseActivity"
            android:exported="false" />
        <activity
            android:name="com.lk.infinitx.workouterz.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>